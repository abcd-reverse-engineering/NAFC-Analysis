<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <corners android:radius="99dp"/>
            <gradient
                android:startColor="#34c759"
                android:endColor="#34c759"
                android:angle="0"
                android:type="linear"
                android:centerColor="#34c759"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#80626379"/>
            <corners android:radius="99dp"/>
            <stroke
                android:width="1dp"
                android:color="#1a000000"/>
        </shape>
    </item>
</selector>
